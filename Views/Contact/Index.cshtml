@{
    ViewData["Title"] = "Porsche - Contact";
    Layout = "_Layout";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Contact Us</title>
    <link rel="stylesheet" href="~/css/Contact.css" asp-append-version="true" />
</head>
<body>
    <div class="text-container">
        <h1>Contact Us :P</h1>
        <br />
        <p><a href="https://www.porsche.com/middle-east/_egypt_/" target="_blank">Our Porsche</a></p>
        <!-- Add social media icons -->
        <div class="social-media">
            <a href="https://www.facebook.com/porsche/" target="_blank"><img src="~/images/contact/facebook.png" alt="Facebook"></a>
            <a href="https://www.instagram.com/Porsche/" target="_blank"><img src="~/images/contact/instagram.png" alt="Instagram"></a>
            <a href="" target="_blank"><img src="~/images/contact/tiktok.png" alt="TikTok"></a>
            <a href="mailto:him@gmail.com"><img src="~/images/contact/email.png" alt="Email"></a>
        </div>
    </div>
    <div class="image-container" id="image1">
        <img src="~/images/contact/porscheContanct.png" alt="Porsche Contact Image">
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.5.1/gsap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.5.1/ScrollTrigger.min.js"></script>
    <script src="~/js/Contact.js"></script> <!-- Link to your JS file -->
</body>

</html>